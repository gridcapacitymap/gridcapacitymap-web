{
  "name": "webgis",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "format": "prettier --no-error-on-unmatched-pattern --write **/*.{ts,tsx,js,jsx,json,css}",
    "formatcheck": "prettier --no-error-on-unmatched-pattern --check **/*.{ts,tsx,js,jsx,json,css}",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 80",
    "preview": "vite preview",
    "generate-client": "wget -O /tmp/openapi.json http://backend:8000/openapi.json && node ./openapi/rewritetags.js /tmp && openapi --input /tmp/openapi.json --output ./src/client --client axios"
  },
  "dependencies": {
    "@types/luxon": "^3.3.2",
    "antd": "^5.10.2",
    "axios": "^1.5.0",
    "luxon": "^3.4.1",
    "maplibre-gl": "^3.1.0",
    "openapi-typescript-codegen": "^0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.15.0",
    "remark-gfm": "^3.0.1",
    "oidc-client-ts": "^2.3.0",
    "react-oidc-context": "^2.3.1",
    "keycloak-js": "^22.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
